{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "YouTube Description Manipulator",
  "version": "1.0",
  "description": "A simple extension to update description of all of your YouTube videos in a playlist in a eye glimpse!",
  "permissions": [
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "background": {
    "scripts": ["jQuery.js", "bScript.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "Start! (Make sure you are on youtube.com signed in)",
	  "default_icon": {
		  "16": "ya.png",
		  "32": "ya.png",
		  "48": "ya.png",
		  "64": "ya.png"
	  }
  },
  "icons": {
		  "16": "ya.png",
		  "32": "ya.png",
		  "64": "ya.png",
		  "128": "ya.png"
	  },
  "manifest_version": 2
}